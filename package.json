{
  "name": "node-typescript-boilerplate",
  "version": "0.0.0",
  "description": "Minimalistic boilerplate to quick-start Node.js development in TypeScript.",
  "engines": {
    "node": ">= 12.13"
  },
  "devDependencies": {
    "@types/ioredis": "^4.22.2",
    "@types/jest": "~25.1.2",
    "@types/mongodb": "^3.3.16",
    "@types/mongoose": "^5.7.1",
    "@types/node": "~12.12.27",
    "@typescript-eslint/eslint-plugin": "~2.19.2",
    "@typescript-eslint/parser": "~2.19.2",
    "concurrently": "^5.3.0",
    "eslint": "~6.8.0",
    "eslint-config-prettier": "~6.10.0",
    "eslint-plugin-jest": "~23.7.0",
    "jest": "~25.1.0",
    "nodemon": "^2.0.7",
    "prettier": "~1.19.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~25.2.0",
    "ts-node-dev": "^1.1.1",
    "tsutils": "^3.18.0",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "clean": "rimraf coverage build tmp",
    "build": "tsc -p tsconfig.release.json",
    "watch:ts": "tsc -w -p tsconfig.release.json",
    "watch:node": "nodemon -r dotenv/config --delay 100ms -q build/src/app.ts",
    "start": "node -r dotenv/config build/src/app.js",
    "start:qa": "npm run watch:node",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch"
  },
  "author": "Jakub Synowiec <github@jakubsynowiec.info>",
  "license": "Apache-2.0",
  "dependencies": {
    "@awaitjs/express": "^0.4.0",
    "apollo-server-fastify": "^2.11.0",
    "aws-sdk": "^2.820.0",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0-rc.5",
    "cloudinary": "^1.23.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "fastify": "^2.13.0",
    "graphql": "^14.6.0",
    "graphql-compose": "^7.14.0",
    "graphql-compose-mongoose": "^7.3.5",
    "ioredis": "^4.19.4",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.20",
    "mongoose": "^5.9.1",
    "ramda": "^0.27.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "showdown": "^1.9.1",
    "tslib": "~1.10.0"
  }
}
