{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": false,
  "quiet": true,
  "watch": [
    "build"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json",
  "delay": "2500",
  "events": {
    "start": "kill-port 8000",
    "restart": "kill-port 8000"
  }
}